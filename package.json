{
  "name": "caching-proxy",
  "version": "1.0.0",
  "description": "A cli tool that starts a caching proxy server, it will forward requests to the actual server and cache the responses. if the same request is made again, it will return the cached response instead of forwarding the request to the server.",
  "main": "src/server.ts",
  "scripts": {
    "test": "npm run test:utils && npm run test:controller",
    "test:utils": "node --experimental-strip-types --test src/util/*.test.ts",
    "test:controller": "NODE_ENV=test node --experimental-strip-types --test src/controllers/*.test.ts",
    "test:specific": "node --experimental-strip-types --test",
    "dev": "node --experimental-strip-types --watch src/server.ts",
    "start": "node --experimental-strip-types src/server.ts",
    "type-check": "tsc --noEmit"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BigFudge420/caching-proxy.git"
  },
  "bin": {
    "caching-proxy": "./src/server.ts"
  },
  "author": "bigfudge",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BigFudge420/caching-proxy/issues"
  },
  "homepage": "https://github.com/BigFudge420/caching-proxy#readme",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "redis": "^5.10.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "@types/yargs": "^17.0.35",
    "nock": "^14.0.10",
    "supertest": "^7.1.4",
    "typescript": "^5.9.3"
  }
}
